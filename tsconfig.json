{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "Node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "types": [
      "vite/client",
      "vite-plugin-svgr/client",
      "cypress",
      "@types/node"
    ],
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src",
    "cypress/**/*.ts",
    "cypress/**/*.cy.ts",
    "cypress/support/commands.ts",
    "cypress/**/*.feature",
    "cypress/global.d.ts",
    "cypress.config.ts"
  ]
}